{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Device Settings",
  "type": "object",
  "properties": {
    "icon": {
      "type": "string",
      "const": "devices"
    },
    "deviceSettings": {
      "type": "object",
      "required": ["endpointId"],
      "properties": {
        "endpointId": {
          "title": "Endpoint client ID",
          "type": "string",
          "examples": ["urn:imei:012345678901234"]
        }
      }
    }
    
  }
}